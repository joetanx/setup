[Unit]
Description=logstash container
Requires=elasticsearch.service
After=elasticsearch.service

[Container]
ContainerName=logstash
Image=docker.io/elastic/logstash:9.2.3
Network=lab.network
PublishPort=1514:1514
PublishPort=5044:5044
Volume=lsdata:/usr/share/logstash:Z
Environment=xpack.monitoring.enabled=false

[Install]
WantedBy=multi-user.target